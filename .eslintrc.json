{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "object-curly-spacing": [2, "always"],
        "semi": 0,
        "max-len": ["error", { "code": 120 }],
        "require-jsdoc": 0,
        "new-cap": 0
    }
}
